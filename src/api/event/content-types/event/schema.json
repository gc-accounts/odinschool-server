{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "about_instructor": {
      "type": "richtext"
    },
    "instructor": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "poster_url": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "integer"
    },
    "duration": {
      "type": "integer"
    },
    "tags": {
      "type": "string"
    },
    "image_url": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "events"
    },
    "is_html": {
      "type": "boolean",
      "default": false
    },
    "is_odin_talk": {
      "type": "boolean",
      "default": false
    }
  }
}
