{
  "kind": "collectionType",
  "collectionName": "webinars",
  "info": {
    "singularName": "webinar",
    "pluralName": "webinars",
    "displayName": "Webinar",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "about_instructor": {
      "type": "richtext"
    },
    "Instructor": {
      "type": "string"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "poster_url": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "duration": {
      "type": "integer"
    },
    "tags": {
      "type": "string"
    },
    "image_url": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "webinars"
    },
    "is_odin_talk": {
      "type": "boolean"
    }
  }
}
